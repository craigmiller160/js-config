{
  "name": "@craigmiller160/js-config",
  "version": "1.0.0-beta",
  "author": "craigmiller160",
  "license": "GPL",
  "bin": {
    "c-build-lib": "./build/bin/c-build-lib.js",
    "c-init": "./build/bin/c-init.js",
    "c-type-check": "./build/bin/c-type-check.js"
  },
  "scripts": {
    "buildScripts": "swc ./scripts --config-file ./configs/swc/.swcrc -C module.type=commonjs -d build",
    "forceBuild": "npm run buildScripts",
    "lint": "eslint --fix --max-warnings=0 '{configs,scripts}/**/*.{js,jsx,mjs,cjs,ts,tsx,mts,cts}'",
    "test": "vitest run",
    "build": "npm run typeCheck && npm run lint && npm run test && npm run forceBuild",
    "typeCheck": "tsc --noEmit --project ./test/tsconfig.json",
    "postinstall": "if test -d scripts; then echo 'No Post-Install Needed'; else node ./build/bin/c-init.js; fi"
  },
  "files": [
    "configs",
    "build"
  ],
  "peerDependencies": {
    "@swc/cli": "^0.1.62",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@relmify/jest-fp-ts": "^2.1.1",
    "@types/node": "^20.5.8",
    "vitest": "^0.34.3"
  },
  "dependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.82",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "fp-ts": "^2.16.1",
    "io-ts": "^2.2.20",
    "io-ts-reporters": "^2.0.1",
    "prettier": "^3.0.3",
    "ts-pattern": "^5.0.5",
    "typescript": "^5.2.2",
    "winston": "^3.10.0"
  }
}

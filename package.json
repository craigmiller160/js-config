{
  "name": "@craigmiller160/js-config",
  "version": "1.0.0-beta",
  "author": "craigmiller160",
  "license": "GPL",
  "publishConfig": {
    "registry": "https://nexus-craigmiller160.ddns.net/repository/npm-private/"
  },
  "bin": {
    "c-build-lib": "./build/bin/c-build-lib.js",
    "c-init": "./build/bin/c-init.js",
    "c-type-check": "./build/bin/c-type-check.js",
    "c-eslint": "./build/bin/c-eslint.js",
    "c-log": "./build/bin/c-log.js",
    "c-test": "./build/bin/c-test.js",
    "c-stylelint": "./build/bin/c-stylelint.js"
  },
  "exports": {
    "./configs/*": {
      "default": "./configs/*"
    }
  },
  "scripts": {
    "buildScripts": "swc ./scripts --config-file ./configs/swc/.swcrc -C module.type=commonjs -d build",
    "forceBuild": "npm run buildScripts",
    "lint": "eslint --fix --max-warnings=0 '{configs,scripts,test}/**/*.{js,jsx,mjs,cjs,ts,tsx,mts,cts}'",
    "test": "vitest run",
    "build": "npm run typeCheck && npm run lint && npm run test && npm run forceBuild",
    "typeCheck": "tsc --noEmit --project ./test/tsconfig.json",
    "postinstall": "if test -f .is-js-config; then echo 'No Post-Install Needed'; else node ./build/bin/c-init.js; fi",
    "prepare": "husky install"
  },
  "files": [
    "configs",
    "build"
  ],
  "peerDependencies": {
    "@swc/cli": "^0.1.62",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@relmify/jest-fp-ts": "^2.1.1",
    "@types/node": "^20.5.8"
  },
  "dependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.82",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-cypress": "^2.14.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sonarjs": "^0.21.0",
    "eslint-plugin-testing-library": "^6.0.1",
    "eslint-plugin-vitest": "^0.3.1",
    "fp-ts": "^2.16.1",
    "husky": "^8.0.3",
    "io-ts": "^2.2.20",
    "io-ts-reporters": "^2.0.1",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "sass": "^1.66.1",
    "stylelint": "^15.10.3",
    "stylelint-config-standard-scss": "^11.0.0",
    "ts-pattern": "^5.0.5",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.3",
    "winston": "^3.10.0"
  }
}
